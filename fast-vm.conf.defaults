## these are defaults that are used when configuring fast-vm
## values are overriden by content of the /etc/fast-vm.conf
VM_PREFIX=fastvm-
LIBVIRT_NETWORK=fastvm-nat
SUBNET_NUMBER=22
THINPOOL_LV=fastvm-pool
THINPOOL_SIZE=50G
FASTVM_GROUP=libvirt
FASTVM_OWNER_ONLY_DELETE=no
FASTVM_APPLIANCE=import
FASTVM_KEYDIST_PASSWORD=testtest
FASTVM_APPLIANCE_URL="http://ftp.linux.cz/pub/linux/people/ondrej_famera/fastvm-images/appliance-1.50.1-$(uname -m).tar.xz"
